<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.creative.gamesdkframe">

    <application
        android:name=".GameApplication"
        android:icon="@mipmap/ic_launcher"
        android:theme="@style/Base.Theme.AppCompat"
        android:label="@string/app_name">
        <activity android:name=".GameSDKMain"
            android:configChanges="fontScale|orientation|keyboardHidden|locale|navigation|screenSize|uiMode"
            android:screenOrientation="sensorLandscape">
            <intent-filter>
                <action android:name="DQCHEN.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>


        <activity android:name="com.creative.gamesdk.common.utils_ui.activity.SplashActivity"
            android:screenOrientation="sensorLandscape"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
            android:configChanges="orientation|screenSize|keyboardHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

    </application>

</manifest>